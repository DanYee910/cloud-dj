$('#sessionuserid').attr('sessionid', <%= @user_id %>);

$("#sc_song_list").empty();
$("#yt_song_list").empty();

<% @streamable_songs.each do |track| %>
  $("#sc_song_list").append('<li> <button class="track_title" id="<%=track.id%>" data-type="sc" data-track-link="<%= track.stream_url %>" data-track-title="<%= track.title %>" data-track-duration="<%= track.duration %>" > <%= track.title %> </button> </li>')
<% end %>

//for soundcloud: track.permalink_url

<% @yt_videos.each do |video| %>
  $("#yt_song_list").append('<li><button class="track_title" id="<%= video.video_id %>" data-type="yt" data-track-link="<%= video.video_id %>" data-track-title="<%= video.title %>" data-track-duration="<%= video.duration %>"><%= video.title %></button></li>')
<% end %>

// attributes of video: http://ruby-doc.org/gems/docs/n/nddrylliog_youtube_it-2.1.4/YouTubeIt/Model/Video.html
// video.video_id for identifier.


